{
  "name": "triangle-app",
  "version": "1.0.0",
  "description": "Desktop application for Triangle",
  "dependencies": {
    "exif-js": "^2.3.0",
    "material-ui": "^0.19.4",
    "pubsub-js": "^1.5.7",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-dropzone": "^4.2.1",
    "react-jss": "^7.2.0",
    "react-scripts": "^1.0.14"
  },
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-forge": "^4.1.2",
    "electron-prebuilt-compile": "1.7.9",
    "foreman": "^2.0.0"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "dev": "nf start -p 3000",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "author": "Endre Simo",
  "license": "MIT",
  "keywords": ["electron", "reactjs", "triangulation"],
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "dmg"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "npm",
        "name": "triangle-app"
      },
      "electronWinstallerConfig": {
        "name": "triangle-app",
        "icon": "src/assets/icons/win/triangle.ico"
      },
      "electronInstallerDMG": {
        "icon": "src/assets/icons/mac/triangle.icns"
      },
      "electronInstallerDebian": {
        "icon": "src/assets/icons/png/128x128.png"
      },
      "electronInstallerRedhat": {
        "icon": "src/assets/icons/png/128x128.png"
      },
      "github_repository": {
        "owner": "esimov",
        "name": "triangle-app"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "triangle-app"
      }
    }
  }
}
